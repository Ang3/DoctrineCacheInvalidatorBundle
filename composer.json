{
    "name": "ang3/doctrine-cache-invalidator-bundle",
    "type": "symfony-bundle",
    "description": "Doctrine cache invalidator.",
    "keywords": ["doctrine", "cache", "invalidator", "invalidate", "invalidation", "second", "level", "cache"],
    "license": "MIT",
    "authors": [
        {
            "name": "Joanis ROUANET",
            "email": "joanis.ang3@gmail.com",
            "homepage": "http://www.lixye.fr"
        }
    ],
    "autoload": {
        "psr-4": { "Ang3\\Bundle\\DoctrineCacheInvalidatorBundle\\": "src/" }
    },
    "require": {
        "php": ">=5.5.9",
        "symfony/framework-bundle": "^3.0 || ^4.0",
        "doctrine/doctrine-bundle": "^1.3",
        "doctrine/orm": "^2.3",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpstan/phpstan-doctrine": "^0.10"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
